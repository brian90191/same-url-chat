{
    "manifest_version": 2,
    "name": "Url NotePad",
    "description": "A extension to note or comment something by the same URL.",
    "version": "0.0.1",
    "options_page": "options.html",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs"
    ],
    "content_security_policy": "script-src  'self' 'unsafe-eval' https://cdn.firebase.com https://*.firebaseio.com https://apis.google.com; object-src 'self'"
}